  "config": {
    "widgetPath": "./tests/TestProjects/Mendix8/widgets",
    "projectPath": "./tests/TestProjects/Mendix8",
    "mendixHost": "http://localhost:8080/",
    "developmentPort": "3000",
    "testProjectId": "25c71ec6-4eb0-4324-8299-4e1948f0c165",
    "testProjects": [
      {
        "revision": 138,
        "branch": "trunk",
