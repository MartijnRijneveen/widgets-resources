  "packagePath": "com.mendix.widget.web",
  "scripts": {
    "start": "concurrently \"npm:start:server\"  \"npm:dev\"",
    "start:server": "pluggable-widgets-tools start:server --open --subprojectPath packages-web/video-player",
    "dev": "pluggable-widgets-tools start:ts -- --subprojectPath packages-web/video-player",
    "build": "pluggable-widgets-tools build:ts --subprojectPath packages-web/video-player",
    "format": "pluggable-widgets-tools format --subprojectPath packages-web/video-player",
    "lint": "eslint --config ../../.eslintrc.js --ext .jsx,.js,.ts,.tsx src/",
    "test": "jest --config ../../scripts/test/jest.web.config.js",
    "pretest:e2e": "cross-env URL=https://videoplayer-sandbox.mxapps.io ../../scripts/test/projectup.sh",
    "test:e2e": "cross-env URL=https://videoplayer-sandbox.mxapps.io/ wdio ../../configs/e2e/wdio.conf.js",
    "test:e2e:dev": "cross-env DEBUG=true wdio ../../configs/e2e/wdio.conf.js",
    "release": "pluggable-widgets-tools release:ts --subprojectPath packages-web/video-player",
    "svncheckout": "ts-node --project ../../scripts/tsconfig.json ../../scripts/svn/CheckoutProject.ts"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@mendix/pluggable-widgets-tools": "8.9.0",
